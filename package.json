{
  "name": "synthwave-name-generator",
  "version": "1.0.0",
  "main": "src/index.pug",
  "author": "Patrick Eriksson <paleite@kth.se>",
  "license": "MIT",
  "scripts": {
    "build":
      "npx parcel build src/index.pug --public-url . --no-source-maps --out-dir docs/",
    "serve": "npx parcel serve src/index.pug",
    "lint": "npx eslint --fix src/*.js; npx stylelint --fix src/scss/*.scss",
    "prettier":
      "npx prettier-eslint --write src/{,**/}.{js,json}; npx prettier-stylelint --write -q src/scss/*.scss",
    "dist":
      "npx rimraf docs/; yarn run prettier && yarn run lint && yarn run build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "normalize-scss": "^7.0.1",
    "parcel-bundler": "^1.7.0",
    "prettier-eslint": "^8.8.1",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.0",
    "stylelint-config-concentric": "^2.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.0.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "pug": "^2.0.3"
  }
}
